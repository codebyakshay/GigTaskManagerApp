{
  "expo": {
    "name": "Gig Task Manager",
    "slug": "Gig Task Manager",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.codebyakshay.gig.taskmanager.app",
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon-nobg.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.codebyakshay.gig.taskmanager.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}